{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "OpenOCD Debug",
            "cwd": "${workspaceFolder}",
            "executable": "${command:cmake.launchTargetPath}",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "serverpath": "C:\\TOOLS\\xpack-openocd-0.11.0-1\\bin\\openocd.exe",
            "device": "STM32F407VG",
            "searchDir": [
                "C:\\TOOLS\\xpack-openocd-0.11.0-1\\scripts"
            ],
            "configFiles": [
                "C:\\TOOLS\\xpack-openocd-0.11.0-1\\scripts\\board\\stm32f4discovery.cfg",
            ],
            "svdFile": "${workspaceFolder}/STM32F407.svd",
            "interface": "swd",
            "postLaunchCommands": [
                "monitor arm semihosting enable"
            ],
        },
        {
            "name": "JLink Debug",
            "cwd": "${workspaceFolder}",
            "executable": "${command:cmake.launchTargetPath}",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "device": "STM32F407VG",
            "interface": "swd",
            "svdFile": "${workspaceFolder}/STM32F407.svd",
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 168000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    {
                        "label": "Output",
                        "type": "console",
                        "port": 0,
                        "encoding": "ascii"
                    }
                ]
            }
        }
    ]
}